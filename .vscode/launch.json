{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Next.js: debug server-side",
        "type": "node-terminal",
        "request": "launch",
        "command": "pnpm deb",
        "sourceMaps": true,
        "sourceMapPathOverrides": {
          "/turbopack/[project]/*": "${webRoot}/*"
        },
        "cwd": "${workspaceFolder}",
        "autoAttachChildProcesses": true,
      }      
    ]
  }